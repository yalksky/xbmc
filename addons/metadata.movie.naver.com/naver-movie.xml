<?xml version="1.0" encoding="euc-kr"?><scraper framework="1.1" date="2012-03-23">
	<NfoUrl dest="3">
		<RegExp input="$$1" output="&lt;url&gt;http://movie.naver.com/movie/bi/mi/detail.nhn?code=\1&lt;/url&gt;" dest="3">
			<expression>movie\.naver\.com[^\?]*\?code=(\d*)</expression>
		</RegExp>
	</NfoUrl>
	<CreateSearchUrl SearchStringEncoding="EUC-KR" dest="3">
		<RegExp input="$$1" output="&lt;url&gt;http://movie.naver.com/movie/search/result.nhn?section=movie&amp;query=\1&lt;/url&gt;" dest="3">
			<expression noclean="1" />
		</RegExp>
	</CreateSearchUrl>
	<GetSearchResults dest="8">
		<RegExp input="$$5" output="&lt;?xml version=&quot;1.0&quot; encoding=&quot;EUC-KR&quot; standalone=&quot;yes&quot;?&gt;&lt;results&gt;\1&lt;/results&gt;" dest="8">
			<RegExp input="$$1" output="&lt;entity&gt;&lt;title&gt;\2 \3&lt;/title&gt;&lt;year&gt;\4&lt;/year&gt;&lt;id&gt;\1&lt;/id&gt;&lt;url&gt;http://movie.naver.com/movie/bi/mi/detail.nhn?code=\1&lt;/url&gt;&lt;/entity&gt;" dest="5">
				<expression repeat="yes" clear="yes">&lt;dt&gt;&lt;a href="/movie/bi/mi/basic.nhn\?code=(\d+)"&gt;&lt;strong&gt;(.*?)&lt;/strong&gt;(.*?)&lt;/a&gt;&lt;/dt&gt;.*?year=\d+"&gt;(\d+)&lt;/a&gt;&lt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;entity&gt;&lt;title&gt;\2 (\3)&lt;/title&gt;&lt;year&gt;\4&lt;/year&gt;&lt;id&gt;\1&lt;/id&gt;&lt;url&gt;http://movie.naver.com/movie/bi/mi/detail.nhn?code=\1&lt;/url&gt;&lt;/entity&gt;" dest="5">
				<expression repeat="yes">&lt;dt&gt;&lt;a href="/movie/bi/mi/basic.nhn\?code=(\d+)"&gt;(.*?)\(&lt;strong&gt;(.*?)&lt;/strong&gt;\)&lt;/a&gt;&lt;/dt&gt;.*?year=\d+"&gt;(\d+)&lt;/a&gt;&lt;</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetSearchResults>
	<GetDetails dest="3">
		<RegExp input="$$8" output="&lt;details&gt;\1&lt;/details&gt;" dest="3">
			<RegExp input="$$2" output="&lt;id&gt;\1&lt;/id&gt;" dest="8">
				<expression />
			</RegExp>
			<RegExp input="$$1" output="&lt;title&gt;\3 (\1)&lt;/title&gt;&lt;year&gt;\2&lt;/year&gt;" dest="8+">
				<expression>&lt;h2 class="title_eng"&gt;\s*([^\,]*)\,\s(\d*)\s*&lt;/h2&gt;\s*&lt;h2 class="title_kor"&gt;(.*?)\s*&lt;/h2&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;title&gt;\3 (\1)&lt;/title&gt;&lt;year&gt;\2&lt;/year&gt;" dest="8+">
				<expression>&lt;h2 class="title_eng"&gt;\s*([^\d]*),\s(\d*)\s*&lt;/h2&gt;\s*&lt;h2 class="title_kor"&gt;(.*?)\s*&lt;/h2&gt;</expression>
			</RegExp>
			<!--For movie does not have english title-->
			<RegExp input="$$1" output="&lt;title&gt;\2&lt;/title&gt;&lt;year&gt;\2&lt;/year&gt;" dest="8+">
				<expression>&lt;h2 class="title_eng"&gt;\s*(\d*)\s*&lt;/h2&gt;\s*&lt;h2 class="title_kor"&gt;(.*?)\s*&lt;/h2&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;title&gt;\2&lt;/title&gt;&lt;year&gt;\2&lt;/year&gt;" dest="8+">
				<expression>&lt;h2 class="title_eng"&gt;\s*(\d*)\s*&lt;/h2&gt;\s*&lt;h2 class="title_kor"&gt;(.*?)\s*&lt;/h2&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;director&gt;\1&lt;/director&gt;" dest="8+">
				<expression repeat="yes">'ifo\.director'[^&gt;]*&gt;(.*?)&lt;/a&gt;</expression>
			</RegExp>
			<RegExp input="$$7" output="&lt;studio&gt;\1&lt;/studio&gt;" dest="8+">
				<RegExp input="$$1" output="\1" dest="7">
					<expression noclean="1">db_text_13.gif(.*)&lt;a name="makingnote"&gt;</expression>
				</RegExp>
				<expression repeat="yes">&lt;li&gt;&lt;a style="text-decoration:none"&gt;([^&gt;]*?)&lt;/a&gt; 영화사&lt;/li&gt;</expression>
			</RegExp>
			<RegExp input="$$7" output="&lt;studio&gt;\1&lt;/studio&gt;" dest="8+">
				<RegExp input="$$1" output="\1" dest="7">
					<expression noclean="1">db_text_13.gif(.*)&lt;a name="makingnote"&gt;</expression>
				</RegExp>
				<expression repeat="yes">\s&lt;li&gt;&lt;a href="/movie/bi/ci/filmo\.nhn\?code=\d+" onClick="clickcr\(this,'crp\.name','\d+','\d+',event\);"&gt;([^&gt;]*)&lt;/a&gt; 제작&lt;/li&gt;</expression>
			</RegExp>
			<RegExp input="$$7" output="&lt;studio&gt;\1&lt;/studio&gt;" dest="8+">
				<RegExp input="$$1" output="\1" dest="7">
					<expression noclean="1">db_text_13.gif(.*)&lt;a name="makingnote"&gt;</expression>
				</RegExp>
				<expression repeat="yes">&lt;li&gt;&lt;a href="/movie/bi/ci/filmo\.nhn\?code=\d+" onClick="clickcr\(this,'crp\.name','\d+','\d+',event\);"&gt;([^&gt;]*?)&lt;/a&gt; 배급&lt;/li&gt;</expression>
			</RegExp>
			<RegExp input="$$7" output="&lt;actor&gt;&lt;name&gt;\2&lt;/name&gt;&lt;role&gt;\3&lt;/role&gt;&lt;thumb&gt;\1&lt;/thumb&gt;&lt;/actor&gt;" dest="8+">
				<RegExp input="$$1" output="\1" dest="7">
					<expression noclean="1">id="cast_1"(.*)id="cast_2"</expression>
				</RegExp>
				<expression repeat="yes" trim="2,3">class="thumb"&gt;&lt;a[^&lt;]*&lt;img src="([^"]*)"[^&lt;]*&lt;/a&gt;&lt;/td&gt;[^&gt;]*&gt;&lt;a[^&gt;]*&gt;([^&lt;]*)&lt;/a&gt;\s*&lt;[^&lt;]*&lt;/span&gt;&lt;br&gt;([^&lt;]*)\s+\S+&lt;/th&gt;</expression>
			</RegExp>
			<RegExp input="$$7" output="&lt;actor&gt;&lt;name&gt;\1&lt;/name&gt;&lt;role&gt;\2&lt;/role&gt;&lt;/actor&gt;" dest="8+">
				<RegExp input="$$1" output="\1" dest="7">
					<expression noclean="1">id="cast_2"(.*)id="cast_3"</expression>
				</RegExp>
				<expression repeat="yes" trim="1,2">&lt;a href[^&gt;]*&gt;([^&lt;]*)&lt;/a&gt;\s*&lt;[^&lt;]*&lt;/span&gt;([^&lt;]*)\s+\S+&lt;/li&gt;</expression>
			</RegExp>
			<RegExp input="$$7" output="&lt;credits&gt;\1&lt;/credits&gt;" dest="8+">
				<RegExp input="$$1" output="\1" dest="7">
					<expression noclean="1">id="staff_1".*?&lt;h6&gt;.*?&lt;h6&gt;(.*)id="staff_2"</expression>
				</RegExp>
				<expression repeat="yes">&lt;li&gt;&lt;a href="/movie/bi/pi/basic\.nhn\?code=\d+" onClick="clickcr\(this,'stf\.name','\d+','\d+',event\);"&gt;([^&gt;]*?)&lt;/a&gt; &lt;span class="en"&gt;[^&gt;]*?&lt;/span&gt; 각본&lt;/li&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;url function=&quot;GetPlot&quot;&gt;http://movie.naver.com/movie/bi/mi/basic.nhn?code=\1&lt;/url&gt;" dest="8+">
				<expression noclean="1">&lt;li&gt;&lt;a href="basic.nhn\?code=(\d+)"&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;genre&gt;\1&lt;/genre&gt;" dest="8+">
				<expression repeat="yes">&lt;a href="/movie/sdb/browsing/bmovie\.nhn\?genre=[^&gt;]*&gt;(.*?)&lt;/a&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;country&gt;\1&lt;/country&gt;" dest="8+">
				<expression repeat="yes">&lt;a href="/movie/sdb/browsing/bmovie\.nhn\?nation=[^&gt;]*&gt;(.*?)&lt;/a&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;runtime&gt;\1&lt;/runtime&gt;" dest="8+">
				<expression>&lt;/em&gt; (\d+)분</expression>
			</RegExp>
			<RegExp input="$$6" output="&lt;premiered&gt;\1&lt;/premiered&gt;" dest="8+">
				<expression repeat="yes">\s개봉 &lt;a href="/movie/sdb/browsing/bmovie\.nhn\?open=[^&gt;]*&gt;(\d+)</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;rating&gt;\1&lt;/rating&gt;" dest="8+">
				<expression>&lt;span class="star"&gt;[^&gt;]*&gt;(.*?)&lt;/em&gt;&lt;/span&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;mpaa&gt;\1&lt;/mpaa&gt;" dest="8+">
				<expression>'ifo\.korgrade'[^&gt;]*&gt;(.*?)&lt;/a&gt;</expression>
			</RegExp>
			<RegExp conditional="EnableVoteNum" input="$$1" output="&lt;votes&gt;\1&lt;/votes&gt;" dest="8+">
				<expression noclean="1">p_rate"&gt;참여 (.*?)명&lt;/p&gt;</expression>
			</RegExp>
			<RegExp input="$$7" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="8+">
				<RegExp input="$$1" output="\1" dest="7">
					<expression noclean="1">&lt;div class="main_poster"&gt;(.*?)&lt;/div&gt;</expression>
				</RegExp>
				<expression>&lt;img src="([^"]*)"</expression>
			</RegExp>
			<RegExp conditional="EnableThumbs" input="$$1" output="&lt;url cache=&quot;naver-photo-$$2.htm&quot; function=&quot;GetNAVERPoster&quot;&gt;http://movie.naver.com/movie/bi/mi/photo.nhn?code=\1&lt;/url&gt;" dest="8+">
				<expression>&lt;a href="photoView\.nhn\?code=(\d+)"</expression>
			</RegExp>
			<!--TMBFanart should be prior to GetNAVERFanart-->
			<RegExp conditional="EnableTMDBFanart" input="$$7" output="&lt;url function=&quot;GetTMDBFanartById&quot;&gt;http://www.google.com/search?q=site:imdb.com\1&lt;/url&gt;" dest="8+">
				<RegExp input="$$1" output="\1 \2" dest="6">
					<expression>&lt;h2 class="title_eng"&gt;\s*(.*?)\,\s*(\d*)\s*&lt;/h2&gt;\s*&lt;h2 class="title_kor"&gt;(.*?)\s*&lt;/h2&gt;</expression>
				</RegExp>
				<RegExp input="$$6" output="+\1" dest="7">
					<expression repeat="yes">([^ ,]+)</expression>
				</RegExp>
				<expression />
			</RegExp>
			<RegExp conditional="EnableFanart" input="$$1" output="&lt;url cache=&quot;naver-photo-$$2.htm&quot; function=&quot;GetNAVERFanart&quot;&gt;http://movie.naver.com/movie/bi/mi/photo.nhn?code=\1&lt;/url&gt;" dest="8+">
				<expression>&lt;a href="photoView\.nhn\?code=(\d+)"</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetDetails>
	<GetPlot dest="4">
		<RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="4">
			<RegExp input="$$1" output="&lt;tagline&gt;\1&lt;/tagline&gt;&lt;plot&gt;\2&lt;/plot&gt;" dest="5">
				<expression>&lt;p class="c_ff4200"&gt;([^&lt;]*)&lt;/p&gt;\s*&lt;p&gt;(.*)&lt;/p&gt;\s*&lt;ul class="other"&gt;</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetPlot>
	<GetNAVERPoster dest="4">
		<RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="4">
			<RegExp input="$$1" output="&lt;url function=&quot;GetNAVERPosterPage&quot;&gt;http://movie.naver.com\1&lt;/url&gt;" dest="5">
				<expression repeat="yes">&lt;a href="([^"]*&amp;page=\d+)"&gt;[^다음]</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetNAVERPoster>
	<GetNAVERPosterPage dest="4">
		<RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="4">
			<RegExp input="$$1" output="&lt;thumb preview=&quot;\1\2&quot;&gt;\1&lt;/thumb&gt;" dest="5">
				<expression repeat="yes">&lt;div class="thumb"&gt;\s*&lt;a href="photoView.nhn[^&lt;]*&lt;img src="([^\?]*)(\?[^\"]*)"</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetNAVERPosterPage>
	<GetNAVERFanart clearbuffers="no" dest="4">
		<RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="4">
			<RegExp input="$$1" output="&lt;url function=&quot;GetNAVERFanartPage&quot;&gt;http://movie.naver.com\1&lt;/url&gt;" dest="5">
				<expression repeat="yes">&lt;a href="([^"]*&amp;page=\d+)"&gt;[^다음]</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetNAVERFanart>
	<GetNAVERFanartPage dest="4">
		<!--append result from GetTMDBFanart/GetNAVERFanartPage calls-->
		<RegExp input="$$11$$5$$10" output="&lt;details&gt;&lt;fanart&gt;\1&lt;/fanart&gt;&lt;/details&gt;" dest="4">
			<RegExp input="$$1" output="&lt;thumb preview=&quot;\1\2&quot;&gt;\1&lt;/thumb&gt;" dest="5">
				<expression repeat="yes" noclean="1">&lt;div class="thumb"&gt;\s*&lt;a href="photoView.nhn[^&lt;]*&lt;img src="([^\?]*)(\?[^\"]*)"</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetNAVERFanartPage>
	<GetTMDBFanartById dest="4">
		<RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="4">
			<RegExp input="$$1" output="&lt;url function=&quot;GetTMDBFanart&quot; cache=&quot;tmdb-images-tt\1.xml&quot;&gt;http://api.themoviedb.org/2.1/Movie.getImages/en/xml/57983e31fb435df4df77afb854740ea9/tt\1&lt;/url&gt;" dest="5">
				<expression>/tt([0-9]+)</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;url function=&quot;GetTMDBFanart&quot; cache=&quot;tmdb-images-\1.xml&quot;&gt;http://api.themoviedb.org/2.1/Movie.getImages/en/xml/57983e31fb435df4df77afb854740ea9/\1&lt;/url&gt;" dest="5">
				<expression>&lt;id&gt;([0-9]*)&lt;/id&gt;</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetTMDBFanartById>
	<GetTMDBFanart clearbuffers="no" dest="5">
		<RegExp input="$$2" output="&lt;details&gt;\1&lt;/details&gt;" dest="5">
			<RegExp input="$$1" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="3">
				<expression repeat="yes" noclean="1">&lt;backdrop[^&gt;]*&gt;(.*?)&lt;/backdrop&gt;</expression>
			</RegExp>
			<RegExp input="$$3" output="&lt;thumb preview=&quot;\2&quot;&gt;\1\3&lt;/thumb&gt;" dest="4">
				<expression repeat="yes" noclean="1,3">&lt;thumb&gt;(.*?)url="([^"]*)" size="poster"(.*?)&lt;/thumb&gt;</expression>
			</RegExp>
			<RegExp input="$$4" output="&lt;thumb\1&gt;\2&lt;/thumb&gt;" dest="11">
				<expression repeat="yes" noclean="1">&lt;thumb([^&gt;]*)&gt;.*?url="([^"]*)" size="original".*?&lt;/thumb&gt;</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetTMDBFanart>
</scraper>